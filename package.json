{
  "name": "ipchanger",
  "version": "1.0.0",
  "description": "An utility program to easily change IP addresses",
  "scripts": {
    "start": "npm run build:live",
    "build:live": "nodemon --ignore Data/*.json --exec ./node_modules/.bin/ts-node -- ./index.ts",
    "build": "tsc",
    "debug": "npm run build && node --nolazy --inspect-brk=9229 ./dist/index.js",
    "gulp": "gulp"
  },
  "dependencies": {
    "body-parser": "^1.17.2",
    "express": "^4.15.4",
    "is-ip": "^2.0.0"
  },
  "devDependencies": {
    "@types/async": "2.0.48",
    "@types/body-parser": "1.16.8",
    "@types/express": "4.11.1",
    "@types/express-serve-static-core": "^4.0.50",
    "@types/is-ip": "^2.0.2",
    "@types/node": "^9.6.1",
    "del": "^3.0.0",
    "es6-promise": "4.2.4",
    "gulp": "^4.0.0",
    "gulp-exec": "^3.0.2",
    "gulp-typescript": "^5.0.0",
    "nodemon": "1.17.3",
    "ts-node": "5.0.1",
    "tslint": "^5.11.0",
    "typescript": "2.8.1"
  }
}
